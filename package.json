  "scripts": {
    "build": "eslint --fix ./src/**/*.ts && webpack --config webpack.prod.js",
    "watch": "webpack -w --config webpack.dev.js",
    "dev": "eslint --fix ./src/**/*.ts && webpack --config webpack.dev.js",
    "dev:rm": "eslint --fix ./src/**/*.ts && npm run dev && npm run remove_comment",
    "remove_comment": "replace-in-file --configFile=remove_comment.js",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "sample": "node samples/server/index.js",
    "fmt": "eslint --fix ./src/**/*.ts",
    "toes6": "lebab --replace src/js/basjoo --transform",
    "doc": "npx typedoc --disableSources"
  },
  "keywords": [],
